{
  "name": "Fullstack Development",
  "dockerComposeFile": [
    "docker-compose.yml"
  ],
  "service": "frontend",
  "workspaceFolder": "/app",
  "remoteUser": "vscode",
  "settings": {
    "terminal.integrated.shell.linux": "/bin/bash",
    "eslint.nodePath": "/app/node_modules/.bin",
    "prettier.prettierPath": "/app/node_modules/.bin/prettier"
  },
  "extensions": [
    "dbaeumer.vscode-eslint",
    "esbenp.prettier-vscode"
  ]
}